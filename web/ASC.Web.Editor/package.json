    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "sass": "^1.39.2",
    "sass-loader": "^12.6.0",
    "serve": "12.0.1",
    "shx": "^0.3.3",
    "source-map-loader": "^3.0.0",
    "style-loader": "3.2.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "5.52.1",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "^4.8.1",
    "webpack-hot-server-middleware": "^0.6.1",
