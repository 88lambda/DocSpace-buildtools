    "@babel/preset-react": "^7.14.5",
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "bundle-loader": "^0.5.6",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "external-remotes-plugin": "^1.0.0",
    "file-loader": "^6.2.0",
